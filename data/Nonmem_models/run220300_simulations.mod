$PROBLEM EXAMPLINIB1
$INPUT ID TIME EVID AMT CMT SS II DV S2_SAMPLING RATE
$DATA data.csv IGNORE=@
$SUBROUTINES ADVAN6 TOL=9

$MODEL
COMP=(CENTRAL) ; 1
COMP = (PERIPH) ; 2
COMP=(AUC) ; 3

$ABBREVIATED COMRES = 2

$PK 

TVVMAX = THETA(1) ;10000
TVVC   = THETA(2) ;70.0
TVKM   = THETA(3) ;2500
TVCL  = THETA(4) ; 4
TVVP = THETA(5)
TVQ = THETA(6)

IF(ICALL == 4) THEN
	VMAX  = TVVMAX* EXP(ETA(1))
	KM    = TVKM * EXP(ETA(3))
	CL = TVCL * EXP(ETA(4))
	VC    = TVVC  * EXP(ETA(2))

	CLEARANCE = CL + (VMAX*1000/KM)

	VP    = TVVP  * EXP(ETA(5))
	Q     = TVQ

	K23 = Q / VC
	K32 = Q / VP
	K20 = CLEARANCE / VC

	A0 = K20 * K32
	A1 = -(K23 + K32 + K20)

	L1 = (- A1 + SQRT(A1*A1 - 4*A0))/2
	L2 = (- A1 - SQRT(A1*A1-4*A0))/2

	HALFLIFE1 = LOG(2)/L1
	HALFLIFE2 = LOG(2)/L2
	
	DOWHILE (HALFLIFE1.GT.25.OR.HALFLIFE2.GT.170.OR.HALFLIFE1.LT.1.OR.HALFLIFE2.LT.6) ; 5x "true" half life
		CALL SIMETA(ETA) 

		VMAX  = TVVMAX* EXP(ETA(1))
		KM    = TVKM * EXP(ETA(3))
		CL = TVCL * EXP(ETA(4))
		VC    = TVVC  * EXP(ETA(2))

		CLEARANCE = CL + (VMAX*1000/KM)

		VP    = TVVP  * EXP(ETA(5))
		Q     = TVQ

		K23 = Q / VC
		K32 = Q / VP
		K20 = CLEARANCE / VC

		A0 = K20 * K32
		A1 = -(K23 + K32 + K20)

		L1 = (- A1 + SQRT(A1*A1 - 4*A0))/2
		L2 = (- A1 - SQRT(A1*A1-4*A0))/2

		HALFLIFE1 = LOG(2)/L1
		HALFLIFE2 = LOG(2)/L2

	ENDDO
ENDIF

K23 = Q / VC
K32 = Q / VP

K20 = CL/VC

S1 = VC/1000 ; dv in mg/l ; amt in mg


IF(NEWIND.LE.1) THEN ; assign negative Cmax Tmax for the new subject 
  COM(1)=-1 ; holder of Cmax 
  COM(2)=-1 ; holder of Tmax
ENDIF

A_0(2) = 0

$DES
DADT(1) = - VMAX*(A(1))/(KM+(A(1)/S1)) - K20*A(1)
DADT(2) = A(1)/S1

CC = A(1)/S1

IF (S2_SAMPLING.GE.30) THEN
   TDOSE = 216
ELSE
   TDOSE = 0
ENDIF

IF(CC.GT.COM(1).AND.T.GE.TDOSE) THEN 
   COM(1)=CC 
   COM(2)=T
ENDIF


$ERROR
CMAX	= COM(1)
IF (S2_SAMPLING.GE.30) THEN
   TMAX = COM(2) - 216
ELSE
   TMAX = COM(2)
ENDIF

AUC = A(2)

DEL=0
IF (F.EQ.0) DEL=1
W=(F+DEL) * THETA(7)
Y=F+W*ETA(6)
IPRED=F
IRES=DV-IPRED
IWRES=IRES/W


$THETA
5 ; VMAX
70.0  ; VC
3500  ; KM
2 ; CL
50 ; VP
4 ; Q
0.22  ; err prop

$OMEGA STANDARD CORRELATION BLOCK(5)
0.45 ; VMAX
0 0.45 ; VC
0 0 0.45 ; KM 
0 0 0 0.45 ; CL
0 0 0 0 0.45 ; VP

$OMEGA 1 FIX

$SIGMA 0 FIX


$SIM ONLYSIM (12345 NEW) SUBPROBLEM=1


$TABLE ID TIME EVID DV S2_SAMPLING PRED IPRED CL VMAX VC KM ETA1 ETA2 ETA3 AUC CMAX TMAX NOPRINT NOAPPEND ONEHEADER FILE=run214.tab FORMAT=,
